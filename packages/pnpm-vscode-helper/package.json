{
  "name": "pnpm-vscode-helper",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "check": "tsc --noEmit",
    "pkg": "vsce package",
    "pub": "vsce publish",
    "pub:patch": "vsce publish patch",
    "pub:minor": "vsce publish minor",
    "pub:major": "vsce publish patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LinbuduLab/vscode-extension-monorepo"
  },
  "bugs": {
    "url": "https://github.com/LinbuduLab/vscode-extension-monorepo/issues"
  },
  "homepage": "https://github.com/LinbuduLab/vscode-extension-monorepo#readme",
  "main": "./dist/index.js",
  "authort": "linbudu599",
  "publisher": "linbudu599",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "command": "extension.helloWorld",
        "key": "ctrl+f10",
        "mac": "cmd+f10",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/title": [],
      "editor/title/context": [],
      "editor/context": [],
      "explorer/context": []
    },
    "snippets": [],
    "viewsContainers": {},
    "views": {},
    "iconThemes": []
  }
}
